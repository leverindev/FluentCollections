<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>leverindev</Authors>
    <PackageId>FluentCollections_leverindev</PackageId>
    <PackageTags>collections</PackageTags>
    <Description>FluentCollections is a small class library with useful collections.</Description>
    <RepositoryUrl>https://github.com/leverindev/FluentCollections</RepositoryUrl>
    <PackageProjectUrl>https://github.com/leverindev/FluentCollections</PackageProjectUrl>
    <PackageLicenseExpression></PackageLicenseExpression>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
